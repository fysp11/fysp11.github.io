    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.3",
    "wrangler": "^4.45.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-alpha.3",
      "@types/react-dom": "npm:types-react-dom@19.0.0",
      "@types/three": "^0.181.0"
    },
    "ignoredBuiltDependencies": [
