    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.552.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.181.0",
