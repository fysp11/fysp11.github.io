{
  "name": "@fysp-eth/personal-website",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fysp11/personal-website.git"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  },
  "scripts": {
    "dev": "pnpm run sync && astro dev",
    "dev:static": "pnpm run sync && astro dev --adapter @astrojs/static",
    "dev:cloudflare": "pnpm exec wrangler dev",
    "start:dev": "astro start",
    "start": "pnpm run dev:cloudflare",
    "build": "pnpm run sync && astro build",
    "build:static": "pnpm run sync && astro build --adapter @astrojs/static",
    "preview": "astro preview",
    "validate": "pnpm run lint && pnpm run type-check",
    "check": "pnpm run validate && pnpm run build",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit && astro check",
    "sync": "astro sync",
    "optimize:images": "node scripts/optimize-images.mjs"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/cloudflare": "^12.6.10",
